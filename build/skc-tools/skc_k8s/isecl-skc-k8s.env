#Kubernetes Distribution microk8s or kubeadm
K8S_DISTRIBUTION=
K8S_MASTER_IP=
K8S_MASTER_HOSTNAME=

# cms
CMS_BASE_URL=https://cms-svc.isecl.svc.cluster.local:8445/cms/v1

# authservice
AAS_API_URL=https://aas-svc.isecl.svc.cluster.local:8444/aas/v1
AAS_API_CLUSTER_ENDPOINT_URL=https://<Master IP>:30444/aas/v1
AAS_ADMIN_USERNAME="admin@aas"
AAS_ADMIN_PASSWORD="aasAdminPass"
AAS_DB_USERNAME="aasdbuser"
AAS_DB_PASSWORD="aasdbpassword"
AAS_DB_HOSTNAME=aasdb-svc.isecl.svc.cluster.local
AAS_DB_PORT="5432"
AAS_DB_NAME=aasdb
AAS_DB_SSLMODE=verify-full
AAS_DB_SSLCERT=/etc/postgresql/server.crt
AAS_SAN_LIST=aas-svc.isecl.svc.cluster.local

# SGX Caching Service
SCS_ADMIN_USERNAME="scs@admin"
SCS_ADMIN_PASSWORD="scsAdminPass"
SCS_DB_USERNAME="scsdbuser"
SCS_DB_PASSWORD="scsdbpassword"
SCS_DB_HOSTNAME=scsdb-svc.isecl.svc.cluster.local
SCS_DB_NAME=pgscsdb
INTEL_PROVISIONING_SERVER=https://sbx.api.trustedservices.intel.com/sgx/certification/v3
INTEL_PROVISIONING_SERVER_API_KEY=9e0153b3f0c948d9ade866635f039e1e
SCS_DB_HOSTNAME=scsdb-svc.isecl.svc.cluster.local
SCS_DB_NAME=pgscsdb
SCS_DB_SSLCERTSRC=/etc/postgresql/server.crt
SCS_DB_PORT="5432"
SCS_BASE_URL=https://scs-svc.isecl.svc.cluster.local:9000/scs/sgx/certification/v1
SCS_CERT_SAN_LIST=scs-svc.isecl.svc.cluster.local

# SGX host verification service
SHVS_ADMIN_USERNAME="shvs@admin"
SHVS_ADMIN_PASSWORD="shvsAdminPass"
SHVS_DB_USERNAME="shvsdbuser"
SHVS_DB_PASSWORD="shvsdbpassword"
SHVS_DB_HOSTNAME=shvsdb-svc.isecl.svc.cluster.local
SHVS_DB_NAME=pgshvsdb
SHVS_CERT_SAN_LIST=shvs-svc.isecl.svc.cluster.local
SHVS_DB_SSLCERTSRC=/etc/postgresql/server.crt
SHVS_DB_PORT="5432"

# SGX Quote Verification Service bootstrap
SQVS_INCLUDE_TOKEN="true"
SQVS_NOSETUP="false"
SGX_TRUSTED_ROOT_CA_PATH=/tmp/trusted_rootca.pem
SQVS_CERT_SAN_LIST=sqvs-svc.isecl.svc.cluster.local

# ihub bootstrap
IHUB_SERVICE_USERNAME="ihub@admin"
IHUB_SERVICE_PASSWORD="ihubAdminPass"
IH_CERT_SAN_LIST=ihub-svc.isecl.svc.cluster.local
# For microk8s
# K8S_API_SERVER_CERT=/var/snap/microk8s/current/certs/server.crt
# For Kubeadm
# K8S_API_SERVER_CERT=/etc/kubernetes/pki/apiserver.crt
K8S_API_SERVER_CERT=
# This is valid for multinode deployment, should be populated once ihub is deployed successfully
IHUB_PUB_KEY_PATH=

# SGX Agent
VALIDITY_DAYS="365"

# KBS bootstrap credentials
KBS_SERVICE_USERNAME="kbs@admin"
KBS_SERVICE_PASSWORD="kbsAdminPass"
SQVS_URL=https://sqvs-svc.isecl.svc.cluster.local:12000/svs/v1
ENDPOINT_URL=https://kbs-svc.isecl.svc.cluster.local:9443/v1
SKC_CHALLENGE_TYPE="SGX"
SESSION_EXPIRY_TIME="60"
KBS_CERT_SAN_LIST=kbs-svc.isecl.svc.cluster.local,<Master IP>,<KBS HOSTNAME>

# ISecl Scheduler
# For microk8s
# K8S_CA_KEY=/var/snap/microk8s/current/certs/ca.key
# K8S_CA_CERT=/var/snap/microk8s/current/certs/ca.crt
# For Kubeadm
# K8S_CA_KEY=/etc/kubernetes/pki/ca.key
# K8S_CA_CERT=/etc/kubernetes/pki/ca.crt
K8S_CA_KEY=
K8S_CA_CERT=

# populate users.env
ISECL_INSTALL_COMPONENTS="AAS,SCS,SHVS,SQVS,SIH,SKBS,SGX_AGENT,SKC-LIBRARY"

SCS_SERVICE_USERNAME="scs@admin"
SCS_SERVICE_PASSWORD="scsAdminPass"

SHVS_SERVICE_USERNAME="shvs@admin"
SHVS_SERVICE_PASSWORD="shvsAdminPass"

SQVS_SERVICE_USERNAME="superadmin"
SQVS_SERVICE_PASSWORD="superAdminPass"

SKC_LIBRARY_USERNAME=skcuser
SKC_LIBRARY_PASSWORD=password
SKC_LIBRARY_CERT_COMMON_NAME=skcuser
SKC_LIBRARY_KEY_TRANSFER_CONTEXT=permissions=nginx,USA

CSP_ADMIN_USERNAME="cspAdminUser"
CSP_ADMIN_PASSWORD="cspAdminPass"

GLOBAL_ADMIN_USERNAME=globalAdminUser
GLOBAL_ADMIN_PASSWORD=globalAdminPass

INSTALL_ADMIN_USERNAME=superadmin
INSTALL_ADMIN_PASSWORD=superAdminPass
