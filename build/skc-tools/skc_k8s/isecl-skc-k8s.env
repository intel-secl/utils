K8S_CONTROL_PLANE_IP= #Control plane ip add
K8S_CONTROL_PLANE_HOSTNAME= #controlplane hostname

AAS_ADMIN_USERNAME= #aas admin user name
AAS_ADMIN_PASSWORD= #aas admin password
CCC_ADMIN_USERNAME= #enterprise admin user name
CCC_ADMIN_PASSWORD= #enterprize admin password
